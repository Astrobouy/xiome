{
	"name": "feature.farm",
	"version": "0.0.0-dev.0",
	"description": "web app factory",
	"license": "ISC",
	"author": "Chase Moskal <chasemoskal@gmail.com>",
	"type": "module",
	"files": [
		"x",
		"s",
		"web"
	],
	"scripts": {
		"prepare": "run-s clean && run-p importmap compile-tsc compile-pug compile-sass && run-s compile-bundle minify stats test",
		"clean": "rimraf x *.pem && mkdirp x",
		"importmap": "importly --host=unpkg < package.json > x/importmap.json",
		"compile-tsc": "tsc",
		"compile-pug": "pug -o x web -O '{\"debug\": false}'",
		"compile-pug-dev": "pug -o x web -O '{\"debug\": true}'",
		"compile-sass": "sass web/:x/",
		"compile-bundle": "rollup --silent --sourcemap -p rollup-plugin-sizes -p @rollup/plugin-node-resolve -f iife -i x/demo.js -o x/demo.bundle.js",
		"minify": "terser x/demo.bundle.js -cm --source-map --comments false -o x/demo.bundle.min.js",
		"watch-tsc": "npm run compile-tsc -s -- --watch",
		"watch-pug": "npm run compile-pug-dev -s -- --watch",
		"watch-sass": "npm run compile-sass -s -- --watch",
		"watch-bundle": "npm run compile-bundle -s -- --watch",
		"watch-tests": "chokidar \"x/**/*.js\" -c \"npm run test -s\"",
		"watch": "run-p watch-tsc watch-pug watch-sass watch-bundle",
		"start": "serve",
		"test": "cynic node --label='feature.farm tests' x/tests.test.js",
		"test-browser": "cynic browser x/tests.test.js --open=true --importmap-path=./x/importmap.json",
		"stats": "ls -lah x/demo.bundle.js x/demo.bundle.min.js"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^10.0.0",
		"@types/gapi.auth2": "0.0.52",
		"@types/js-yaml": "^3.12.5",
		"@types/koa": "^2.11.4",
		"@types/koa-bodyparser": "^4.3.0",
		"@types/koa-mount": "^4.0.0",
		"@types/koa-static": "^4.0.1",
		"@types/koa__cors": "^3.0.2",
		"@types/mongodb": "^3.5.27",
		"@types/node": "^14.14.3",
		"@types/pug": "^2.0.4",
		"chokidar-cli": "^2.1.0",
		"cynic": "0.1.0-dev.10",
		"feather-icons": "^4.28.0",
		"importly": "0.1.0-dev.0",
		"mkdirp": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"octicons": "^8.5.0",
		"pug-cli": "^1.0.0-alpha6",
		"rimraf": "^3.0.2",
		"rollup": "^2.32.1",
		"rollup-plugin-sizes": "^1.0.3",
		"sass": "^1.27.0",
		"serve": "^11.3.2",
		"terser": "^5.3.8",
		"typescript": "^4.0.3"
	},
	"dependencies": {
		"@adobe/lit-mobx": "^1.0.0",
		"@koa/cors": "^3.1.0",
		"@stripe/stripe-js": "^1.9.0",
		"chalk": "^4.1.0",
		"commotion": "^0.1.0-dev.0",
		"crosscall": "0.0.6-dev.21",
		"google-auth-library": "^6.1.3",
		"js-yaml": "^3.14.0",
		"js-yaml-chase-esm": "^3.14.0-esm.2",
		"koa": "^2.13.0",
		"koa-bodyparser": "^4.3.0",
		"koa-logger": "^3.2.1",
		"koa-mount": "^4.0.0",
		"koa-static": "^5.0.0",
		"lit-element": "^2.4.0",
		"lit-html": "^1.3.0",
		"menutown": "0.0.1",
		"mobx": "^5.15.7",
		"mobx-lit-element": "^0.1.2",
		"mongodb": "^3.6.2",
		"pug": "^3.0.0",
		"redcrypto": "0.0.0-dev.7",
		"renraku": "0.0.0-dev.31",
		"stripe": "^8.106.0",
		"unique-names-generator": "^4.3.1"
	},
	"keywords": [
		"web-components"
	],
	"repository": {
		"type": "git",
